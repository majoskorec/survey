{
    "name": "majoskorec/survey",
    "description": "Symfony Survey Application",
    "license": "proprietary",
    "type": "project",
    "require": {
        "php": "^8.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "doctrine/dbal": "^4.3.4",
        "doctrine/doctrine-bundle": "^3.0",
        "doctrine/doctrine-migrations-bundle": "^3.6",
        "doctrine/orm": "^3.5.7",
        "easycorp/easyadmin-bundle": "^4.27.2",
        "phpdocumentor/reflection-docblock": "^5.6.3",
        "phpstan/phpdoc-parser": "^2.3",
        "symfony/asset": "~7.3.0",
        "symfony/asset-mapper": "~7.3.5",
        "symfony/console": "~7.3.6",
        "symfony/doctrine-messenger": "~7.3.6",
        "symfony/dotenv": "~7.3.2",
        "symfony/expression-language": "~7.3.2",
        "symfony/flex": "^2.9.0",
        "symfony/form": "~7.3.6",
        "symfony/framework-bundle": "~7.3.6",
        "symfony/http-client": "~7.3.6",
        "symfony/intl": "~7.3.5",
        "symfony/mailer": "~7.3.5",
        "symfony/mailgun-mailer": "~7.3.1",
        "symfony/mime": "~7.3.4",
        "symfony/monolog-bundle": "^3.10",
        "symfony/notifier": "~7.3.3",
        "symfony/process": "~7.3.4",
        "symfony/property-access": "~7.3.3",
        "symfony/property-info": "~7.3.5",
        "symfony/runtime": "~7.3.4",
        "symfony/security-bundle": "~7.3.4",
        "symfony/security-csrf": "~7.3.0",
        "symfony/serializer": "~7.3.5",
        "symfony/stimulus-bundle": "^2.31",
        "symfony/string": "~7.3.4",
        "symfony/translation": "~7.3.4",
        "symfony/twig-bundle": "~7.3.4",
        "symfony/uid": "~7.3.1",
        "symfony/ux-turbo": "^2.31",
        "symfony/validator": "~7.3.7",
        "symfony/web-link": "~7.3.0",
        "symfony/yaml": "~7.3.5",
        "twig/cssinliner-extra": "^3.22",
        "twig/extra-bundle": "^3.22",
        "twig/inky-extra": "^3.22",
        "twig/twig": "^3.22.0"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.48.2",
        "friendsofphp/php-cs-fixer": "^3.89.2",
        "friendsoftwig/twigcs": "^6.5",
        "jonmldr/grumphp-doctrine-task": "dev-feat/add-em-option as 3.1",
        "php-parallel-lint/php-parallel-lint": "^1.4",
        "phpro/grumphp": "^2.17",
        "phpstan/extension-installer": "^1.4.3",
        "phpstan/phpstan": "^2.1.32",
        "phpstan/phpstan-doctrine": "^2.0.11",
        "phpstan/phpstan-symfony": "^2.0.8",
        "phpunit/phpunit": "^12.4.2",
        "pixelfederation/coding-standards": "^5.0",
        "psalm/plugin-symfony": "^5.2.7",
        "roave/security-advisories": "dev-latest",
        "symfony/browser-kit": "~7.3.6",
        "symfony/css-selector": "~7.3.6",
        "symfony/debug-bundle": "~7.3.5",
        "symfony/maker-bundle": "^1.64",
        "symfony/stopwatch": "~7.3.0",
        "symfony/web-profiler-bundle": "~7.3.5",
        "vimeo/psalm": "^6.13.1",
        "weirdan/doctrine-psalm-plugin": "^2.10"
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/majoskorec/grumphp-doctrine-task.git"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "ergebnis/composer-normalize": true,
            "php-http/discovery": true,
            "phpro/grumphp": true,
            "phpstan/extension-installer": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "bump-after-update": true,
        "sort-packages": true
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "7.3.*"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "importmap:install": "symfony-cmd"
        }
    }
}
